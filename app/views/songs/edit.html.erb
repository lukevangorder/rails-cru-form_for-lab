<h3>Song Edit Form</h1>
<%= form_for(@song) do |f| %>
    <label>Song name:</label><br>
    <%= f.text_field :name %><br>
    <label>Artist:</label><br>
    <%= f.text_area :artist_id %><br>
    <label>Genre:</label><br>
    <%= f.text_area :genre_id %><br>
    <%= f.submit "Update Song" %>
<% end %>